{
    "name": "maer/enstart-skeleton",
    "description": "Skeleton app for maer/enstart framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Magnus Eriksson",
            "email": "mange@reloop.se"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "maer/enstart": "^0.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpcs --standard=PSR2 --extensions=php --ignore=vendor/,config.local.php,logs/,public/ -n -s ./"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
